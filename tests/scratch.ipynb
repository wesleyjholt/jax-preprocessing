    "from functools import partial\n",
    "from jax import jit\n",
    "\n",
    "def f(x, a, b):\n",
    "    return a * x + b\n",
    "\n",
    "f1 = jit(partial(f, a=2, b=3))\n",
    "f2 = jit(partial(f, a=4, b=5))\n",
    "\n",
    "for i in range(5):\n",
    "    print(f1(1.0))\n",
    "\n",
    "for i in range(5):\n",
    "    print(f2(1.0))"
   ]
